package java_calisma;

public class Dörtİşlem {
public static void main(String[] args) {
	
	int a = 20;
	int b = 5;
	
	int topla = a + b;
	System.out.println(topla);
	
	int farkAl = a - b;
	System.out.println(farkAl);
	
	int çarp = a * b;
	System.out.println(çarp);
	
	int böl = a / b;
	System.out.println(böl);
	
	int bölkalan = a % b;
	System.out.println(bölkalan);
}
}
